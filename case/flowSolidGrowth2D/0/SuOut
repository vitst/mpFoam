FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      SuOut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            zeroGradient;
        value           $internalField;
    }
    right
    {
        type            zeroGradient;
        value           $internalField;
    }
    bottom
    {
        type            zeroGradient;
        value           $internalField;
    }
    top
    {
        type            zeroGradient;
        value           $internalField;
    }
    frontAndBack
    {
        type            empty;
    }
//    back
//    {
//        type           zeroGradient;
//	    value	       $internalField;
//    }
}

