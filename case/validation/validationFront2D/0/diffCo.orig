FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      diffCo;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type           zeroGradient;
    }
    outlet
    {
        type           zeroGradient;

    }
    top
    {
        type           symmetry;
    }
    bottom
    {
        type           symmetry;
    }
    frontAndBack
    {
        type           empty;
    }
//    back
//    {
//        type           zeroGradient;
//    }
}

