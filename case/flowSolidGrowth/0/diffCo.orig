FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      diffCo;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1E-6;

boundaryField
{
    left
    {
        type           zeroGradient;
    }
    right
    {
        type           zeroGradient;

    }
    bottom
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    front
    {
        type           zeroGradient;
    }
    back
    {
        type           zeroGradient;
    }
}

